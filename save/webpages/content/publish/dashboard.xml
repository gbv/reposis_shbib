<?xml version="1.0" encoding="UTF-8"?>

<MyCoReWebPage>
  <section title="Arbeitshilfen" xml:lang="de"/>
  <section title="Dashboard" xml:lang="en"/>
  <section xml:lang="all">
    <script src='../../js/searchDuplicates.js'> </script>
    <script src='../../js/checkObjectIndex.js'> </script>
    
    <h1> Arbeitshilfen </h1>
    
    <h2> PPN Duplikate </h2>
    <div class="row">
      <p>
        Liste aller PPN die in mehr als einem Titel vorkommen: 
      </p>
      <div data-duplicate-searchfield="recordIdentifier" style="height: 300px;width: 600px;overflow-y: scroll;border: solid thin black;"> 
       <div style="text-align:center;color:gray;" ><i class="fa fa-spinner" aria-hidden="true"></i> </div>
      </div>
    </div>
    
    <h2> nicht indexierte Aufnahmen </h2>
    <div class="row">
      <p>
        Alle Titel die vorhanden sind, aber nicht indexiert wurden.
      </p>
      <div id="nonindexedTitles" style="height: 300px;width: 600px;overflow-y: scroll;border: solid thin black;">
        <div style="text-align:center;color:gray;" ><i class="fa fa-spinner" aria-hidden="true"></i> </div>  
      </div>
    </div>

    
  </section>
</MyCoReWebPage>
